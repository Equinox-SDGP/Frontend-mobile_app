{
  "expo": {
    "name": "Equinox",
    "slug": "Frontend-mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.equinox"
    },
    "android": {
      "package": "com.equinox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "dev-lupmiwit5z3hexny.us.auth0.com"
        }
      ],
      "expo-router",
      "expo-font",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "Equinox",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ef4984e-dba5-43b4-a58c-edec8b7264b0"
      }
    }
  }
}
